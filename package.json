{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.75.1"
  },
  "scripts": {
    "start": "env-cmd ../config/security.env.local react-scripts start",
    "build-dev": "env-cmd ../config/security.env.dev react-scripts build",
    "precommit": "lint-staged",
    "lint": "eslint src/**/*.js",
    "prettier": "prettier --write \"src/**/*.{js,css,json}\"",
    "prettier-check": "prettier --list-different \"src/**/*.{js,css,json}\"",
    "test": "react-scripts test --env=jsdom --no-watchman",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "babel-eslint": "7",
    "env-cmd": "^7.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "husky": "0.14.3",
    "lint-staged": "4.2.3",
    "prettier": "1.7.4",
    "react-test-renderer": "^16.1.1"
  }
}
